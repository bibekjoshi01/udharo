{
  "cli": {
    "version": ">= 8.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "credentialsSource": "remote",
      "env": { "NODE_ENV": "development" }
    },
    "preview": {
      "distribution": "internal",
      "credentialsSource": "remote",
      "env": { "NODE_ENV": "production" }
    },
    "production": {
      "credentialsSource": "remote",
      "env": { "NODE_ENV": "production" }
    }
  }
}
